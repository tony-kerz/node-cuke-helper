{
  "name": "cuke-helpr",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tony-kerz/node-cuke-helpr.git"
  },
  "scripts": {
    "cuke": "cross-env DEBUG=test:*,app:* cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register ./test",
    "cuke-feature": "cross-env DEBUG=test:*,app:* cucumber-js --compiler=js:babel-polyfill --compiler=js:babel-core/register",
    "pretest": "xo",
    "test": "npm run cuke",
    "prebuild": "npm run test",
    "build": "babel ./src -d ./dist"
  },
  "author": "tony kerz",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "config": "^1.25.1",
    "debug": "^2.6.1",
    "express-jwt": "^5.1.0",
    "helpr": "^0.12.0",
    "lodash": "^4.17.4",
    "test-helpr": "^0.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "body-parser": "^1.16.1",
    "cross-env": "^3.1.4",
    "cucumber": "^2.2.0",
    "express": "^4.14.1",
    "xo": "^0.17.1"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "no-console": "error",
      "no-unused-expressions": [
        "error",
        {
          "allowShortCircuit": true,
          "allowTernary": true
        }
      ],
      "unicorn/no-new-buffer": "off",
      "unicorn/filename-case": "off",
      "unicorn/custom-error-definition": "off",
      "unicorn/no-array-instanceof": "off",
      "unicorn/catch-error-name": "off",
      "unicorn/no-process-exit": "off",
      "unicorn/throw-new-error": "off",
      "unicorn/number-literal-case": "off",
      "unicorn/prefer-starts-ends-with": "off",
      "unicorn/prefer-type-error": "off",
      "unicorn/explicit-length-check": "off",
      "unicorn/no-abusive-eslint-disable": "off",
      "space-before-function-paren": "off"
    }
  }
}
